{
  "extends": [
    "config:base",
    ":automergeAll",
    ":masterIssue",
    ":semanticCommits",
    ":timezone(Europe/Paris)",
    "docker:enableMajor",
    "group:allNonMajor",
    "schedule:earlyMondays"
  ],
  "masterIssueAutoclose": true,
  "node": {
    "supportPolicy": [
      "active"
    ]
  },
  "postUpdateOptions": [
    "npmDedupe",
    "yarnDedupeHighest"
  ],
  "rangeStrategy": "pin"
}
