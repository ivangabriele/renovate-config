{
  "extends": [
    "config:base",
    ":preserveSemverRanges",
    ":semanticCommits",
    ":timezone(Europe/Paris)",
    "docker:enableMajor",
    "group:allNonMajor",
    "schedule:earlyMondays"
  ],
  "semanticCommitType": "build",
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "semanticCommitScope": "dev-deps"
    }
  ]
}
